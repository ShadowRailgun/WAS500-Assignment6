<div class="bookimg">
    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>name</th>
                <th>author</th>
                <th>edit</th>
                <th>delete</th>
            </tr>
        </thead>
        <% Books.forEach(s=>
            { %>
            <tr>
                <td><%= s.id %></td>
                <td><%= s.name %></td>
                <td><%= s.author %></td>
                <td>Edit</td>
                <td>Delete</td>
            </tr>
    </table>
</div>
<% }); %>